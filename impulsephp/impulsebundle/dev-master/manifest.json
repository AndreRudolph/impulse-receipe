{
    "aliases": ["impulse", "impulse-php", "impulsephp", "impulse-bundle", "impulsebundle"],
    "bundles": {
        "Impulse\\ImpulseBundle\\ImpulseBundle": ["all"]
    },
    "copy-from-package": {
        "src/Resources/config/packages/": "%CONFIG_DIR%/packages/",
        "src/Resources/assets/": "assets/",
        "src/Resources/skeleton/installation/src/": "%SRC_DIR%/",
        "src/Resources/skeleton/installation/templates/": "templates/",
        "webpack_impulse.config.js": "webpack_impulse.config.js"
    },
    "composer-scripts": {
        "cache:clear": "symfony-cmd"
    }
}